#1.Write R program to call the (built-in) dataset airquality. For each numerical columns calculate mean, median, max, min values.
#print (mean(airquality$ozone))


print(airquality)
cat("\n","Mean of each column in airquality dataset","\n")

print(mean(airquality$Ozone))
print(mean(airquality$Solar.R))
print(mean(airquality$Wind))
print(mean(airquality$Temp))
print(mean(airquality$Month))
print(mean(airquality$Day))

print("Median of each column")

print(median(airquality$Ozone))
print(median(airquality$Solar.R))
print(median(airquality$Wind))
print(median(airquality$Temp))
print(median(airquality$Month))
print(median(airquality$Day))

print("max of each column")
print(max(airquality$Ozone))
print(max(airquality$Solar.R))
print(max(airquality$Wind))
print(max(airquality$Temp))
print(max(airquality$Month))
print(max(airquality$Day))


print("Min of each column")
print(min(airquality$Ozone))
print(min(airquality$Solar.R))
print(min(airquality$Wind))
print(min(airquality$Temp))
print(min(airquality$Month))
print(min(airquality$Day))